# Azure OpenAI
AOAI_ENDPOINT=https://<your-azure-openai-host>.openai.azure.com/
AOAI_API_KEY=<Azure-OpenAI-APIKey>

# These names will come from your model deployments in your Azure OpenAI resource
AOAI_COMPLETIONS_MODEL=<Completions model deployment ID, ie: text-davinci-003>
AOAI_CHAT_COMPLETIONS_MODEL=<Chat completions model deployment ID, ie gpt-4>
AOAI_EMBEDDINGS_MODEL=<Embeddings model deployment ID>

# Azure OpenAI endpoints with a Whisper model deployed.
AOAI_ENDPOINT_WHISPER=https://<your-azure-openai-host>.openai.azure.com/
AOAI_API_KEY_WHISPER=<Azure-OpenAI-APIKey>
AOAI_MODEL_WHISPER=<whisper-model>

# public OpenAI
OPENAI_ENDPOINT=https://api.openai.com/v1
OPENAI_API_KEY=<OpenAI-APIKey>
OPENAI_COMPLETIONS_MODEL=<Completions model, ie: text-davinci-003>
OPENAI_CHAT_COMPLETIONS_MODEL=<Chat completions model, ie: gpt-4>
OPENAI_EMBEDDINGS_MODEL=<Embeddings model>

# These are used in the BYOD scenario.
COGNITIVE_SEARCH_API_KEY=<Cognitive-Search-APIKey>
COGNITIVE_SEARCH_API_ENDPOINT=https://<your-cognitive-server-host>.search.windows.net
COGNITIVE_SEARCH_API_INDEX=<Cognitive-Search-Index>

# enable testing with TokenCredentials
USE_TOKEN_CREDS=true
